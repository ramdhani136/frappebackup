 "html": "      {% if doc.workflow_state==\"Pending\" %}\r\n    <div class=\"dn_title\">BUKTI CHECKER BARANG MASUK</div>\r\n    <table class=\"dn_title_table\">\r\n      <tbody>\r\n        <tr>\r\n          <td style=\"width: 20%\">\r\n            <b>NO. BCBM</b> : {{ doc.name }} <br />\r\n            <b>Tanggal</b> : {{frappe.utils.formatdate(doc.posting_date,\"dd MMMM YYYY\")}}<br />\r\n          </td>\r\n          <td style=\"width: 23%\">\r\n            <b>Nomor PO</b> : {{doc.items[0].purchase_order}}<br />\r\n            <b>Supplier</b> : {{doc.supplier}} <br />\r\n          </td>\r\n          <td style=\"width: 35%\">\r\n            <b>Alamat</b> :{{doc.supplier_address}}  <br />\r\n            <b>Nopol</b> :  <br />\r\n            <br />\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n    <table cellspacing=\"0\" class=\"dn_list_table\">\r\n      <thead>\r\n        <tr>\r\n          <th style=\"width: 3%\">NO</th>\r\n          <th style=\"width: 12%; text-align: center\">KODE</th>\r\n          <th style=\"width: 18%; text-align: center\">ITEM</th>\r\n          <th style=\"width: 7%; text-align: center\">DN</th>\r\n          <th style=\"width: 7%; text-align: center\">REAL</th>\r\n          <th style=\"width: 6%; text-align: center\">SATUAN</th>\r\n          <th style=\"width: 45%; text-align: center\">KETERANGAN HASIL CHECKER </th>\r\n          <th style=\"width: 17%; text-align: center\">Stoker</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        {%- for row in doc.items -%}\r\n        <tr>\r\n          <td style=\"text-align: center\">{{row.idx}}</td>\r\n          <td>{{row.item_code}}</td>\r\n          <td>{{row.item_name}}</td>\r\n         <td style=\"text-align: center\">{{\"{:,.0f}\".format(row.received_qty)}}</td>\r\n          <td style=\"text-align: center\"></td>\r\n          <td style=\"text-align: center\">{{row.uom}}</td>\r\n          <td></td>\r\n          <td></td>\r\n        </tr>\r\n        {%- endfor -%}\r\n        <tr>\r\n          <td\r\n            colspan=\"8\"\r\n            style=\"\r\n              background-color: rgb(241, 241, 241);\r\n              text-align: center;\r\n              height: 25px;\r\n              border-bottom: solid 1.4px black;\r\n              font-weight: bold;\r\n              font-size: 1em;\r\n            \"\r\n          >\r\n            Catatan\r\n          </td>\r\n        </tr>\r\n        <tr>\r\n          <td\r\n            colspan=\"8\"\r\n            style=\"\r\n              height: 50px;\r\n              border-top: none;\r\n              border-bottom: solid 1.4px black;\r\n            \"\r\n          ></td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <table class=\"dn_sign_table\" cellspacing=\"0\">\r\n      <thead>\r\n        <tr>\r\n          <th style=\"text-align: center\">Checker</th>\r\n          <th style=\"text-align: center\">Kabag.Gudang</th>\r\n          <th style=\"text-align: center\">Fakturing</th>\r\n          <th style=\"text-align: center\">Supir</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr>\r\n          <td></td>\r\n          <td></td>\r\n          <td></td>\r\n          <td></td>\r\n       \r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n      {% endif %}\r\n    ",
